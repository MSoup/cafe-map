{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "cli": {
      "defaultCollection": "@nativescript/schematics",
      "analytics": false
    },
    "projects": {
      "cafe-map": {
        "root": "",
        "sourceRoot": ".",
         "projectType": "application",
         "prefix": "app",
         "schematics": {
          "@schematics/angular:component": {
            "style": "scss"
          }
         }
      }
    },
    "defaultProject": "cafe-map"
  }